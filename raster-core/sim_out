S_0x5995e2ab6520 .scope task, "wait_for_completion" "wait_for_completion" 2 229, 2 229 0, S_0x5995e2a8e1b0;
 .timescale -9 -10;
TD_tb_raster_core.wait_for_completion ;
    %vpi_call 2 231 "$display", "Time %0t: Waiting for rasterization to complete...", $time {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5995e2ab6860_0, 0, 32;
T_1.0 ;
    %load/vec4 v0x5995e2ab70c0_0;
    %nor/r;
    %load/vec4 v0x5995e2ab6860_0;
    %cmpi/s 1000, 0, 32;
    %flag_get/vec4 5;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz T_1.1, 8;
    %wait E_0x5995e2a64050;
    %load/vec4 v0x5995e2ab6860_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5995e2ab6860_0, 0, 32;
    %jmp T_1.0;
T_1.1 ;
    %load/vec4 v0x5995e2ab6860_0;
    %cmpi/s 1000, 0, 32;
    %flag_inv 5; GE is !LT
    %jmp/0xz  T_1.2, 5;
    %vpi_call 2 238 "$display", "ERROR: Rasterization did not complete within 1000 cycles" {0 0 0};
    %jmp T_1.3;
T_1.2 ;
    %vpi_call 2 240 "$display", "Time %0t: Rasterization completed in %0d cycles", $time, v0x5995e2ab6860_0 {0 0 0};
T_1.3 ;
    %end;
    .scope S_0x5995e2a7ca20;
T_2 ;
    %wait E_0x5995e2a617b0;
    %load/vec4 v0x5995e2ab4a80_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x5995e2ab4540_0, 0;
    %pushi/vec4 0, 0, 1;
